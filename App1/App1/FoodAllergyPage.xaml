<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.FoodAllergyPage">
    <ContentPage.Content>
        <StackLayout Padding="50">

            <Label Text="Food Allergy" FontSize="Title"/>
            <Label Text="Do you have food allergy? "/>
            <Label Text="Please select your answer"/>
            <Button Text="None"/>
            <Button Text="Eggs"/>
            <Button Text="Nuts"/>
            <Button Text="Seafood"/>
            <Button Text="Next" x:Name="Page8to9" Clicked="Page8to9_Clicked" HorizontalOptions="End" BackgroundColor="White"/>


        </StackLayout>
    </ContentPage.Content>
</ContentPage>